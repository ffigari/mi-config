[user]
	name = Francisco Figari
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[init]
	defaultBranch = master
[core]
	editor = vim
[alias]
	tree = log --graph --decorate --pretty=oneline --abbrev-commit
	trea = log --graph --decorate --pretty=oneline --abbrev-commit --all
	fetc = fetch --atomic --tags --prune --prune-tags
[difftool "vimdiff"]
	cmd = vimdiff \"$LOCAL\" \"$REMOTE\"
